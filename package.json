{
  "name": "chameleon-chess",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "all": "run-s format check test",
    "build": "esbuild --bundle --outdir=dist src/index.tsx",
    "check": "tsc --noEmit",
    "format": "prettier --write src",
    "test": "jest"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "esbuild": "^0.20.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "prettier": {
    "importOrder": [
      "^../",
      "^./"
    ],
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  }
}
